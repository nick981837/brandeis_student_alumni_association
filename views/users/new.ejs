<section id="section-user">
    <form action="/users/create" method="POST" id="user-form" class="card">
      <h1>Sign Up</h1>
        <div class="row">
          <div class="col-sm-4">
      <label for="inputFirstName">Name</label>
      <input
        type="text"
        name="name"
        id="inputName"
        placeholder="Name"
        autofocus
        required
      />
      </div>
      <div class="col-sm-4">
      <label for="inputEmail">Email</label>
      <input
        type="text"
        name="email"
        id="inputEmail"
        placeholder="Email"
        required
      />
      </div>
      <div class="col-sm-4">
      <label for="inputEmail">Password</label>
      <input
        type="text"
        name="password"
        id="inputPassword"
        placeholder="Password"
        required
      />
      </div>
      <div class="col-sm-4">
      <label for="inputRole">Role</label>
      <br/>
      <select name="role" id="role">
        <option value="student">Alumni</option>
        <option value="alumni">Student</option>
      </select>
      </div>
      <div class="col-sm-4">
      <label for="inputGraduationYear">GraduationYear</label>
      <input
        type="number"
        name="graduationYear"
        id="inputGraduationYear"
        placeholder="Graduation Year"
        required
      />
      </div>
      <div class="col-sm-4">
      <label for="inputMajor">Major</label>
      <input
        type="text"
        name="major"
        id="inputMajor"
        placeholder="Major"
        required
      />
      </div>
      <div class="col-sm-4">
      <label for="inputJob">Job</label>
      <input
        type="text"
        name="job"
        id="inputJob"
        placeholder="Job"
      />
      </div>
      <div class="col-sm-4">
      <label for="inputCompany">Company</label>
      <input
        type="text"
        name="company"
        id="inputCompany"
        placeholder="Company"
      />
      </div>
      <div class="col-sm-4">
      <label for="inputCity">City</label>
      <input
        type="text"
        name="city"
        id="inputCity"
        placeholder="City"
      />
      </div>
      <div class="col-sm-4">
      <label for="inputState">State</label>
      <input
        type="text"
        name="state"
        id="inputState"
        placeholder="State"
      />
      </div>
      <div class="col-sm-4">
      <label for="country">Country</label>
      <input
        type="text"
        name="country"
        id="inputCountry"
        placeholder="Country"
      />
      </div>
      <div class="col-sm-4">
      <label for="inputZipCode">Zip Code</label>
      <input
        type="text"
        name="zipCode"
        id="inputZipCode"
        placeholder="Zip Code"
      />
      </div>
      <label for="inputBio">Bio</label>
      <textarea
        name="bio"
        id="inputBio"
      >
    </textarea>
      <label for="inputInterests">Interests</label>
      <textarea id="inputInterests" name="interests">Seperate with comma</textarea>
    </div>
      <br/>
      <div><input type="submit" value="submit"></input></div>
      <a href="/users/login">Already a User? Login</a>
    </form>
</section>
  